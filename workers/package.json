{
  "name": "icon-search-workers",
  "version": "1.0.0",
  "description": "Cloudflare Workers API for icon search",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:production": "wrangler deploy --env production",
    "tail": "wrangler tail",
    "kv:create": "wrangler kv:namespace create ICON_CACHE",
    "kv:create:preview": "wrangler kv:namespace create ICON_CACHE --preview"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240806.0",
    "wrangler": "^3.65.1",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "hono": "^4.5.3"
  }
}
